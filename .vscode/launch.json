{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "karma: Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "address": "localhost",
      "restart": true,
      "timeout": 600000,
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "pathMapping": {
        "base": "${workspaceFolder}/src"
      }
    }
  ]
}
